<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:clipToOutline="true"
    android:background="@drawable/bg_btsheet_round15"
    android:paddingBottom="49dp"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/disease_bottom_sheet"
        android:clipToOutline="true"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <View
            android:id="@+id/view1"
            android:layout_marginTop="10dp"
            android:background="@drawable/ic_grabber"
            android:layout_width="56dp"
            android:layout_height="5dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"/>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:paddingLeft="16dp"
            android:paddingRight="16dp"
            android:clickable="true"
            android:id="@+id/di_layout_0"
            android:paddingTop="10dp"
            android:paddingBottom="10dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginTop="30dp">

            <ImageView
                android:id="@+id/ic_di_0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:background="@drawable/ic_disease_0"/>

            <TextView
                android:layout_marginLeft="15dp"
                android:id="@+id/tv_di_0"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:includeFontPadding="false"
                android:fontFamily="@font/notosanskrsemibold"
                android:textSize="15sp"
                android:textColor="@color/black"
                app:layout_constraintStart_toEndOf="@id/ic_di_0"
                app:layout_constraintTop_toTopOf="@id/ic_di_0"
                android:text="고지혈증"/>

            <TextView
                android:layout_marginLeft="15dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:includeFontPadding="false"
                android:fontFamily="@font/notosanskrregular"
                android:textSize="11sp"
                android:textColor="#898989"
                app:layout_constraintTop_toBottomOf="@id/tv_di_0"
                app:layout_constraintStart_toEndOf="@id/ic_di_0"
                app:layout_constraintBottom_toBottomOf="@id/ic_di_0"
                android:text="혈중 지질 수치가 높은 질환"/>
            
            <View
                android:visibility="invisible"
                android:id="@+id/check_0"
                android:layout_width="21dp"
                android:layout_height="21dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                android:background="@drawable/ic_mint_check"/>

        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:paddingLeft="16dp"
            android:paddingRight="16dp"
            android:id="@+id/di_layout_1"
            android:clickable="true"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toBottomOf="@id/di_layout_0"
            android:paddingTop="10dp"
            android:paddingBottom="10dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent">

            <ImageView
                android:id="@+id/ic_di_1"

                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:background="@drawable/ic_disease_1"/>

            <TextView
                android:layout_marginLeft="15dp"
                android:id="@+id/tv_di_1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:includeFontPadding="false"
                android:fontFamily="@font/notosanskrsemibold"
                android:textSize="15sp"
                android:textColor="@color/black"
                app:layout_constraintStart_toEndOf="@id/ic_di_1"
                app:layout_constraintTop_toTopOf="@id/ic_di_1"
                android:text="고혈압"/>

            <TextView
                android:layout_marginLeft="15dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:includeFontPadding="false"
                android:fontFamily="@font/notosanskrregular"
                android:textSize="11sp"
                android:textColor="#898989"
                app:layout_constraintTop_toBottomOf="@id/tv_di_1"
                app:layout_constraintStart_toEndOf="@id/ic_di_1"
                app:layout_constraintBottom_toBottomOf="@id/ic_di_1"
                android:text="혈압이 지속적으로 높은 질환"/>

            <View
                android:visibility="invisible"
                android:id="@+id/check_1"
                android:layout_width="21dp"
                android:layout_height="21dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                android:background="@drawable/ic_mint_check"/>

        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:paddingLeft="16dp"
            android:paddingRight="16dp"
            android:id="@+id/di_layout_2"
            android:clickable="true"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toBottomOf="@id/di_layout_1"
            android:paddingTop="10dp"
            android:paddingBottom="10dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent">

            <ImageView
                android:id="@+id/ic_di_2"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:background="@drawable/ic_disease_2"/>

            <TextView
                android:layout_marginLeft="15dp"
                android:id="@+id/tv_di_2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:includeFontPadding="false"
                android:fontFamily="@font/notosanskrsemibold"
                android:textSize="15sp"
                android:textColor="@color/black"
                app:layout_constraintStart_toEndOf="@id/ic_di_2"
                app:layout_constraintTop_toTopOf="@id/ic_di_2"
                android:text="당뇨"/>

            <TextView
                android:layout_marginLeft="15dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:includeFontPadding="false"
                android:fontFamily="@font/notosanskrregular"
                android:textSize="11sp"
                android:textColor="#898989"
                app:layout_constraintTop_toBottomOf="@id/tv_di_2"
                app:layout_constraintStart_toEndOf="@id/ic_di_2"
                app:layout_constraintBottom_toBottomOf="@id/ic_di_2"
                android:text="혈당 조절이 어려워 혈당 수치가 높은 질환"/>

            <View
                android:visibility="invisible"
                android:id="@+id/check_2"
                android:layout_width="21dp"
                android:layout_height="21dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                android:background="@drawable/ic_mint_check"/>

        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:paddingLeft="16dp"
            android:paddingRight="16dp"
            android:id="@+id/di_layout_3"
            android:clickable="true"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toBottomOf="@id/di_layout_2"
            android:paddingTop="10dp"
            android:paddingBottom="10dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent">

            <ImageView
                android:id="@+id/ic_di_3"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:background="@drawable/ic_disease_3"/>

            <TextView
                android:layout_marginLeft="15dp"
                android:id="@+id/tv_di_3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:includeFontPadding="false"
                android:fontFamily="@font/notosanskrsemibold"
                android:textSize="15sp"
                android:textColor="@color/black"
                app:layout_constraintStart_toEndOf="@id/ic_di_3"
                app:layout_constraintTop_toTopOf="@id/ic_di_3"
                android:text="심혈관질환"/>

            <TextView
                android:layout_marginLeft="15dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:includeFontPadding="false"
                android:fontFamily="@font/notosanskrregular"
                android:textSize="11sp"
                android:textColor="#898989"
                app:layout_constraintTop_toBottomOf="@id/tv_di_3"
                app:layout_constraintStart_toEndOf="@id/ic_di_3"
                app:layout_constraintBottom_toBottomOf="@id/ic_di_3"
                android:text="심장과 혈관에 영향을 미치는 질환"/>

            <View
                android:visibility="invisible"
                android:id="@+id/check_3"
                android:layout_width="21dp"
                android:layout_height="21dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                android:background="@drawable/ic_mint_check"/>

        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:paddingLeft="16dp"
            android:paddingRight="16dp"
            android:id="@+id/di_layout_4"
            android:clickable="true"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toBottomOf="@id/di_layout_3"
            android:paddingTop="10dp"
            android:paddingBottom="10dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent">

            <ImageView
                android:id="@+id/ic_di_4"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:background="@drawable/ic_disease_4"/>

            <TextView
                android:layout_marginLeft="15dp"
                android:id="@+id/tv_di_4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:includeFontPadding="false"
                android:fontFamily="@font/notosanskrsemibold"
                android:textSize="15sp"
                android:textColor="@color/black"
                app:layout_constraintStart_toEndOf="@id/ic_di_4"
                app:layout_constraintTop_toTopOf="@id/ic_di_4"
                android:text="호흡기질환"/>

            <TextView
                android:layout_marginLeft="15dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:includeFontPadding="false"
                android:fontFamily="@font/notosanskrregular"
                android:textSize="11sp"
                android:textColor="#898989"
                app:layout_constraintTop_toBottomOf="@id/tv_di_4"
                app:layout_constraintStart_toEndOf="@id/ic_di_4"
                app:layout_constraintBottom_toBottomOf="@id/ic_di_4"
                android:text="호흡기에 장기적인 문제가 생기는 질환"/>

            <View
                android:visibility="invisible"
                android:id="@+id/check_4"
                android:layout_width="21dp"
                android:layout_height="21dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                android:background="@drawable/ic_mint_check"/>

        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:paddingLeft="16dp"
            android:paddingRight="16dp"
            android:id="@+id/di_layout_5"
            android:clickable="true"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toBottomOf="@id/di_layout_4"
            android:paddingTop="10dp"
            android:paddingBottom="10dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent">

            <ImageView
                android:id="@+id/ic_di_5"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:background="@drawable/ic_disease_5"/>

            <TextView
                android:layout_marginLeft="15dp"
                android:id="@+id/tv_di_5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:includeFontPadding="false"
                android:fontFamily="@font/notosanskrsemibold"
                android:textSize="15sp"
                android:textColor="@color/black"
                app:layout_constraintStart_toEndOf="@id/ic_di_5"
                app:layout_constraintTop_toTopOf="@id/ic_di_5"
                android:text="기타"/>

            <TextView
                android:layout_marginLeft="15dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:includeFontPadding="false"
                android:fontFamily="@font/notosanskrregular"
                android:textSize="11sp"
                android:textColor="#898989"
                app:layout_constraintTop_toBottomOf="@id/tv_di_5"
                app:layout_constraintStart_toEndOf="@id/ic_di_5"
                app:layout_constraintBottom_toBottomOf="@id/ic_di_5"
                android:text="기타 질병에 해당하는 모든 질환"/>

            <View
                android:visibility="invisible"
                android:id="@+id/check_5"
                android:layout_width="21dp"
                android:layout_height="21dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                android:background="@drawable/ic_mint_check"/>

        </androidx.constraintlayout.widget.ConstraintLayout>

        <View
            android:id="@+id/di_view"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@id/di_layout_5"
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:background="#D9D9D9"
            android:layout_marginTop="11dp"
            />

        <TextView
            android:clickable="true"
            android:id="@+id/btn_done"
            android:paddingTop="11dp"
            android:paddingBottom="11dp"
            android:layout_marginLeft="16dp"
            android:layout_marginRight="16dp"
            android:includeFontPadding="false"
            android:fontFamily="@font/notosanskrmedium"
            android:textSize="15sp"
            android:layout_marginTop="29dp"
            android:text="완료"
            android:gravity="center"
            app:layout_constraintTop_toBottomOf="@id/di_view"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textColor="@color/white"
            android:backgroundTint="#1E54DF"
            android:background="@drawable/bg_btn_10_gray"/>

    </androidx.constraintlayout.widget.ConstraintLayout>



</androidx.constraintlayout.widget.ConstraintLayout>